<div class="mx-auto" style="width: 80em;">

<h1> <%= current_user.username %>'s Watchlist</h1>

  <div class="row row-cols-1 row-cols-md-4 g-4 mx-5">
    <% current_user.watched_listings.each do | listing | %>
      <div class="col mb-4">
       <%= render "watch", listing: listing %>
    <% end %>
      </div>
    </div>
  </div>